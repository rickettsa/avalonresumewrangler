{
    "resume": {
        "properties": {
            "Competency": {
                "type": "nested",
                "properties": {
                    "name": { "type": "string" },
                    "years": { "type": "string" }
                }
            },
            "EducationHistory": {
                "type": "nested",
                "properties": {
                    "DegreeMajor": { "type": "string" },
                    "DegreeName": { "type": "string" },
                    "SchoolName": { "type": "string" },
                    "startDate": {
                        "type": "date",
                        "format": "dateOptionalTime"
                    }
                }
            },
            "EmploymentHistory": {
                "type": "nested",
                "properties": {
                    "EmployerOrgName": { "type": "string" },
                    "Position": {
                        "properties": {
                            "Description": { "type": "string" },
                            "OrganizationName": { "type": "string" },
                            "StartDate": { "type": "string" },
                            "Title": { "type": "string" },
                            "positionType": { "type": "string" }
                        }
                    }
                }
            },
            "cell": { "type": "string" },
            "email": { "type": "string" },
            "firstName": { "type": "string" },
            "lastName": { "type": "string" }
        }
    }
}
